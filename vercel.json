{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "lessmanual.ai"
        }
      ],
      "destination": "https://www.lessmanual.ai/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "lessmanual.ai"
        }
      ],
      "destination": "https://www.lessmanual.ai/:path*",
      "permanent": true
    }
  ]
}
